          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
      <BarChart fx:id="pickUpBC" animated="false" layoutX="20.0" layoutY="524.0" prefHeight="293.0" prefWidth="560.0" title="PicK ups from Madison,Brooklyn">
