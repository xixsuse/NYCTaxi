<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="825.0" prefWidth="1121.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="demo.MainDashBoardController">
   <children>
      <BarChart fx:id="tripsNoChart" animated="false" layoutX="574.0" layoutY="7.0" prefHeight="313.0" prefWidth="547.0" title="No of trip for each taxi Type">
