        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
      <BarChart fx:id="minutesBC" animated="false" layoutX="574.0" layoutY="296.0" prefHeight="313.0" prefWidth="547.0" title="Minutes per trip for each taxi type">
